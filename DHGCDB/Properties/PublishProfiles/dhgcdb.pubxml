<?xml version="1.0" encoding="utf-8"?>
<!--
This file is used by the publish/package process of your Web project. You can customize the behavior of this process
by editing this MSBuild file. In order to learn more about this please visit http://go.microsoft.com/fwlink/?LinkID=208121. 
-->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://localhost:82</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>localhost</MSDeployServiceURL>
    <DeployIisAppPath>dhgcdb</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=localhost;Initial Catalog=aspnet-dhgclientdb_iis;Integrated Security=True" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="DHGCDB.Models.ApplicationDbContext, DHGCDB" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="ClientDBContext" Order="2" Enabled="False">
          <Destination Path="Data Source=localhost;Initial Catalog=aspnet-dhgclientdb_iis;Integrated Security=True" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="DHGCDB.DAL.ClientDBContext, DHGCDB" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="PersonDBContext" Order="3" Enabled="False">
          <Destination Path="Data Source=localhost;Initial Catalog=aspnet-dhgclientdb_iis;Integrated Security=True" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="DHGCDB.Models.PersonDBContext, DHGCDB" MigrationConfiguration="DHGCDB.Migrations.Configuration, DHGCDB" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)ClientDBContext-Web.config Connection String">
      <ParameterValue>Data Source=localhost;Initial Catalog=aspnet-dhgclientdb_iis;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=localhost;Initial Catalog=aspnet-dhgclientdb_iis;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)PersonDBContext-Web.config Connection String">
      <ParameterValue>Data Source=localhost;Initial Catalog=aspnet-dhgclientdb_iis;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>